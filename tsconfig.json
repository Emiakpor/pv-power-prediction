{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-native",
		"lib": ["DOM","ESNext"],
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": false,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/assets/*": ["./assets/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
	"exclude": ["node_modules"]
}
